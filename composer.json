{
	"name": "david63/lockatend",
	"type": "phpbb-extension",
	"description": "This extension will place any locked topics at the end of the forums topic list, also works in the MCP.",
	"version": "3.3.0",
	"time": "2021-02-07",
	"keywords": [
		"phpbb",
		"extension",
		"topic",
		"locked"
	],
	"license": "GPL-2.0-only",
	"authors": [
		{
			"name": "David Wood",
			"role": "Mod convertor"
		},
		{
			"name": "UseLess",
			"role": "Original Mod developer"
		}
	],
	"require": {
        "php": ">=7.1.3",
        "composer/installers": "~1.0"
    },
	"extra": {
		"display-name": "[d63] Locked topics at end",
		"soft-require": {
			"phpbb/phpbb": ">=3.3.0,<4.0.0@dev"
      },
		"version-check": {
        	"host": "version.phpbbdev.space",
        	"directory": "/versions",
        	"filename": "lock_at_end_version_file.json",
			"ssl": false
    	}
	}
}
